        "use_env_prefix": true
      },
      "email_template": {
        "subject": "Your {NAME} Verification Code",
        "from_name": "{NAME} Customer Service"
      },
      "features": {
        "allow_order_cancellation": false,
        "allow_order_modification": false,
        "require_otp_for_returns": false
      },
      "fallback_options": {
        "allow_legacy_email_order_auth": true,
        "auto_suggest_otp": true
      },
      "rate_limiting": {
        "max_otp_requests_per_hour": 10,
        "cooldown_between_requests_seconds": 30
      }
    }
  },
